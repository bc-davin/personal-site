{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build:prod",
  "outputDirectory": "client/dist/client-app",
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.py"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
